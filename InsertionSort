def insertionSort(A):
    print A
    for j in range (1,len(A)):
        print 'Pega o '+str(A[j])+ ' para comparar'
        chave = A[j]
        i = j -1
        while i >= 0 and A[i] > chave:
            A[i+1] = A[i]
            i = i - 1
            print A
        print 'troca'
        A[i+1] = chave
        print A



elements = [5,2,4,6,1,3]
insertionSort(elements)